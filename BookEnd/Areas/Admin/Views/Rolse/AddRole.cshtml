@model BookEnd.Models.ViewModels.RoleManagerViewModel
@{

    ViewData["Title"] = "AddRole";
    Layout = "/Views/Shared/_Layout.cshtml";
}

    <form method="post" asp-action="AddRole">
        <div class="card">
            <div class="card-header text-white bg bg-dark">
                اضافه کردن نقش
            </div>
            <div class="card-body col-md-6">
                <div class="form-group">
                    <input class="form-control" asp-for="DiscriptionR" placeholder="  توضیحات نقس را وارد کنید " />
                </div>
                <div class="form-group">
                    <input class="form-control" asp-for="NameR" placeholder="نام نقس را وارد کنید" />
                </div>
                <div class="form-group">
                    <input class="btn btn-primary col-md-3" type="submit" value="تایید" />
                </div>
            </div>
            </div>

</form>
